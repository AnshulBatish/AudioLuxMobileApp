{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router"
      ],
      [
        "react-native-wifi-reborn"
      ],
      [
        "expo-font",
        {
          "fonts": [
            {
              "asset": "assets/fonts/Kumbh_Sans/KumbhSans-VariableFont_YOPQ,wght.ttf"
            }
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "name": "AudioLuxMobileApp",
    "slug": "AudioLuxMobileApp",
    "ios": {
      "bundleIdentifier": "com.anshulbat.AudioLuxMobileApp"
    }
  },
  "ios": {
    "entitlements": {
      "com.apple.developer.networking.wifi-info": true
    }
  }
}
